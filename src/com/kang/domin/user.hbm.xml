<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping package="com.kang.domin">
    
    	<class name="User" table="sys_user">
    		
    		<id name="id" column="user_id">
    			
    			<generator class="native"></generator>
    			
    		</id>
    		
    		<property name="code" column="user_code"></property>
    		
    		<property name="name" column="user_name"></property>
    		
    		<property name="password" column="user_password"></property>
    		
    		<property name="state" column="user_state"></property>
    		
    	
    	</class>
    
    </hibernate-mapping>